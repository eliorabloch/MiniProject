<Page x:Class="PL.OrderListPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PL"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="OrderListPage">

    <Grid>
        <Grid HorizontalAlignment="Left" Height="451" Margin="-1,-1,0,0" VerticalAlignment="Top" Width="801">
            <Label Content="Suggestions" HorizontalAlignment="Left" Height="26" Margin="569,0,0,0" VerticalAlignment="Top" Width="116"/>
            <Label Content="My Orders" HorizontalAlignment="Left" Height="26" Margin="132,0,0,0" VerticalAlignment="Top" Width="90" RenderTransformOrigin="0.5,0.5">
                <Label.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="0.003"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Label.RenderTransform>
            </Label>
            <ListView x:Name="MyOrderListView" HorizontalAlignment="Left" Height="425" Margin="0,26,0,0" VerticalAlignment="Top" Width="394">
                <ListView.View>
                    <GridView>
                        <GridViewColumn/>
                    </GridView>
                </ListView.View>
            </ListView>
            <ListView x:Name="SuggestionListView" HorizontalAlignment="Left" Height="425" Margin="412,26,0,0" VerticalAlignment="Top" Width="389">
                <ListView.View>
                    <GridView>
                        <GridViewColumn/>
                    </GridView>
                </ListView.View>
            </ListView>
        </Grid>

    </Grid>
</Page>
